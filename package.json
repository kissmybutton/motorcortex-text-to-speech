{
  "name": "@kissmybutton/motorcortex-video",
  "version": "2.2.2",
  "main": "dist/motorcortex-video.cjs.js",
  "module": "dist/motorcortex-video.esm.js",
  "browser": "dist/motorcortex-video.umd.js",
  "repository": "https://github.com/kissmybutton/motorcortex-video",
  "author": "KissMyButton PC (kissmybutton.gr) <opensource@kissmybutton.gr>",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "update:packages": "npm update --save/--save-dev",
    "concurrently": "concurrently -c \"cyan.bold,magenta.bold\" --names \"JS,Styles\"",
    "lint": "eslint -c .eslintrc src/**/*.js",
    "lint:fix": "npm:lint -- --fix",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "rollup -c",
    "start": "npm run build:lib && concurrently -c \"cyan.bold,magenta.bold\" \"npm:build:lib -- -w\"  \"npm:start:demo\" ",
    "start:demo": "webpack serve --config ./demo/webpack.config.js --mode=development --progress ",
    "build:demo": "webpack --mode=production --config ./demo/webpack.config.js",
    "test:prod": "npm run lint",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "prebuild": "rimraf dist",
    "prepare": "husky install"
  },
  "keywords": [
    "motorcortex",
    "video",
    "motorcortex-plugin"
  ],
  "lint-staged": {
    "*.{json,md,yml,yaml,css}": [
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.22.6",
    "@babel/core": "7.22.6",
    "@babel/preset-env": "7.22.6",
    "@donkeyclip/motorcortex": "9.9.0",
    "@donkeyclip/motorcortex-player": "2.10.8",
    "@rollup/plugin-json": "4.1.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.3.0",
    "browserslist": "4.21.9",
    "caniuse-lite": "1.0.30001512",
    "concurrently": "7.6.0",
    "coveralls": "3.1.1",
    "css-loader": "6.8.1",
    "es6-promise": "4.2.8",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-standard": "5.0.0",
    "exports-loader": "4.0.0",
    "husky": "8.0.3",
    "imports-loader": "4.0.1",
    "json-stringify-safe": "5.0.1",
    "lint-staged": "13.2.3",
    "npx": "10.2.2",
    "prettier": "2.8.8",
    "rimraf": "3.0.2",
    "rollup": "2.79.1",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "7.0.2",
    "shelljs": "0.8.5",
    "webpack": "5.88.1",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.15.1",
    "whatwg-fetch": "3.6.2"
  },
  "peerDependencies": {
    "@donkeyclip/motorcortex": "^9.1.5"
  }
}
